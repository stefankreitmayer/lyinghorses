$('#score').text('Score: '+<%= @player.score %>)
$('#feedback').text('<%= @is_correct ? 'Correct answer' : 'Wrong answer' %>')
<%- if @expired %>
 window.location.replace('/rooms/'+<%= @room.id %>)
<%- else %>
setTimeout(function(){
 $.getScript('<%= "#{room_path}/get_feedback" %>');
 }, 1000
);
<%- end %>
